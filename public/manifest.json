{
  "name": "tab",
  "version": "1.0",
  "description": "一个tab",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "chrome_url_overrides": {
    "newtab": "./tab.html"
  },
  "optional_permissions": [
    "background",
    "contextMenus",
    "geolocation",
    "management",
    "topSites",
    "bookmarks",
    "unlimitedStorage",
    "topSites",
    "identity",
    "http://*/",
    "https://*/",
    "favicon",
    "history",
    "alarms",
    "notifications",
    "tabs",
    "storage",
    "activeTab",
    "declarativeContent",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "scripting"
  ],
  "host_permissions": ["*://*/*"],
  "web_accessible_resources": [],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/vite.svg",
      "32": "/vite.svg",
      "48": "/vite.svg",
      "128": "/vite.svg"
    },
    "default_title": "React CRX MV3"
  },
  "icons": {
    "16": "/vite.svg",
    "32": "/vite.svg",
    "48": "/vite.svg",
    "128": "/vite.svg"
  },
  "externally_connectable": {
    "matches": ["http://127.0.0.1:5173/"]
  },
  "permissions": ["http://*/", "https://*/"],
  "content_security_policy": "connect-src: https://push2.eastmoney.com https://searchadapter.eastmoney.com; script-src 'self' 'unsafe-eval' https://searchadapter.eastmoney.com; object-src 'self';"
}
